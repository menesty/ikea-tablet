<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                    android:color="#ff9ad5ff" />
            <stroke
                    android:width="1dp"
                    android:color="#ff377fba" />
            <padding
                    android:left="10dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp" />
        </shape>
    </item>
    <item >
        <shape >

            <solid
                    android:color="#E8E8E8" />
            <padding
                    android:left="10dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp" />
        </shape>
    </item>
    <item android:right="1dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ffdd2c23" />
        </shape>
    </item>
</selector>